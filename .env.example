# Vanilla GenAI Stack - Environment Variables Example
# Copy this file to .env and fill in the values

# Docker Compose Configuration
COMPOSE_PROJECT_NAME=vanilla-genai

# Service Endpoints (for external services)
# Uncomment and set these when using external services
# OLLAMA_ENDPOINT=
# POSTGRES_HOST=
# NEO4J_HOST=

# Ollama Configuration
OLLAMA_MODELS=llama3

# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DB=vanilla_genai

# Neo4j Configuration
NEO4J_USER=neo4j
NEO4J_PASSWORD=

# API Keys (keep these empty in .env.example)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# FastAPI Configuration
FASTAPI_PORT=8000
FASTAPI_HOST=0.0.0.0

# Web UI Configuration
WEB_UI_PORT=3000